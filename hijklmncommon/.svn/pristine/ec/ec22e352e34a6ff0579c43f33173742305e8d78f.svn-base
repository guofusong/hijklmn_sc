package edu.guosong.sc.hijklmncommon.util;

public class ConvertUtil {

	public static Integer str2intE(String target) throws Exception {
		try {
			return Integer.parseInt(target);
		} catch (Exception e) {
			throw new Exception(e);
		}
	}

	public static Integer str2int(String target) {
		try {
			return Integer.parseInt(target);
		} catch (Exception e) {
			return 0;
		}
	}

}
